<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>OnlineParserTest</servlet-name>
		<servlet-class>com.xitxer.uateam.notification.server.OnlineParserTest</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OnlineParserTest</servlet-name>
		<url-pattern>/online_parser_test</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>OnlineParserAndSendEmailTest</servlet-name>
		<servlet-class>com.xitxer.uateam.notification.server.OnlineParserAndSendEmailTest</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OnlineParserAndSendEmailTest</servlet-name>
		<url-pattern>/online_parser_and_send_email_test</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CronSimpleTask</servlet-name>
		<servlet-class>com.xitxer.uateam.notification.server.CronSimpleTask</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CronSimpleTask</servlet-name>
		<url-pattern>/cron_simple_task</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CronSimpleTaskTest</servlet-name>
		<servlet-class>com.xitxer.uateam.notification.server.CronSimpleTaskTest</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CronSimpleTaskTest</servlet-name>
		<url-pattern>/cron_simple_task_test</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<security-constraint>
		<web-resource-collection>
			<url-pattern>/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>admin</role-name>
		</auth-constraint>
	</security-constraint>
</web-app>
